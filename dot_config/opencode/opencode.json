{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "oh-my-opencode@v2.14.0",
    "opencode-openai-codex-auth",
    "opencode-wakatime"
  ],
  "mcp": {
    "call-me": {
      "enabled": false,
      "type": "local",
      "command": ["npm", "--prefix", "~/code_projects/personal/call-me/server", "run", "start"],
      "timeout": 60000,
      "environment": {
        "CALLME_PHONE_PROVIDER": "telnyx"
      }
    }
  },
  "provider": {}
}
